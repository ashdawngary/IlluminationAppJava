<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.neelb.sampleapp.MainScreenGood"
    tools:layout_editor_absoluteY="81dp"
    tools:layout_editor_absoluteX="0dp">

    <Button
        android:id="@+id/TaskListButton"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:layout_marginEnd="116dp"
        android:background="@drawable/roundrect"
        android:fontFamily="sans-serif"
        android:text="Task List"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        android:layout_marginStart="116dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="98dp"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="116dp"
        android:layout_marginRight="116dp"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/storyButton"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/roundrect"
        android:fontFamily="sans-serif"
        android:text="Stories"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/musicButton"
        android:layout_marginTop="13dp"
        app:layout_constraintTop_toBottomOf="@+id/musicButton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/musicButton"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/musicButton"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/roundrect"
        android:fontFamily="sans-serif"
        android:text="Music"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/TaskListButton"
        android:layout_marginTop="14dp"
        app:layout_constraintTop_toBottomOf="@+id/TaskListButton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/TaskListButton"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/animalVideosButton"
        android:layout_width="140dp"
        android:layout_height="48dp"
        android:background="@drawable/roundrect"
        android:fontFamily="sans-serif"
        android:text="Animal Videos"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="10dp"
        app:layout_constraintTop_toBottomOf="@+id/deepbreathing"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="parent" />

    <Button
        android:id="@+id/deepbreathing"
        android:layout_width="0dp"
        android:layout_height="48dp"
        android:background="@drawable/roundrect"
        android:fontFamily="sans-serif"
        android:text="Breathing"
        android:textAllCaps="false"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        app:layout_constraintRight_toRightOf="@+id/storyButton"
        android:layout_marginTop="11dp"
        app:layout_constraintTop_toBottomOf="@+id/storyButton"
        tools:layout_constraintLeft_creator="1"
        app:layout_constraintLeft_toLeftOf="@+id/storyButton"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/backButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="6dp"
        android:background="@drawable/roundrect"
        android:text="Back"
        android:textAllCaps="false"
        android:textColor="#FF0000"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginBottom="-2dp"
        android:layout_marginRight="2dp" />

    <ImageView
        android:id="@+id/Toplogo"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:scaleType="fitXY"
        app:srcCompat="@drawable/illuminationlogo"
        tools:layout_constraintTop_creator="1"
        tools:layout_constraintRight_creator="1"
        tools:layout_constraintBottom_creator="1"
        app:layout_constraintBottom_toTopOf="@+id/TaskListButton"
        android:layout_marginStart="7dp"
        android:layout_marginEnd="7dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginTop="4dp"
        tools:layout_constraintLeft_creator="1"
        android:layout_marginBottom="14dp"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginLeft="7dp"
        android:layout_marginRight="7dp" />

</android.support.constraint.ConstraintLayout>
